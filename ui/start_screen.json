      },
      {
        "label_panel": {
          "type": "panel",
          "size": [ "100%c", "100%" ],
          "controls": [
            {
              "gamertag_label": {
                "type": "label",
                "color": "$main_header_text_color",
                "layer": 1,
                "text": "#gamertag_label",
                "locked_alpha": 0.5,
                "size": [ "default", "100%" ],
                "offset": [ 0, "25%" ],
                "bindings": [
                  {
                    "binding_name": "#gamertag_label"
                  }
